{
  "name": "Medicine-Admin",
  "version": "0.0.0",
  "description": "医学实验室管理后台前端",
  "scripts": {
    "prettier": "node_modules/.bin/prettier -.prettierrc -config ./ --loglevel silent --write \"./**/*.js\"",
    "build": "npm run prettier && npm run build:client && npm run build:server",
    "start:backend": "node test/backend.js",
    "start:dev": "node cli/dev.js",
    "start:prod": "npm run build && node dist/ssr/bundle.js",
    "build:client": "webpack --config build/webpack.client.prod.js",
    "build:server": "webpack --config build/webpack.server.prod.js"
  },
  "dependencies": {
    "antd": "^3.8.1",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "^2.26.0",
    "css-loader": "^0.28.11",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "prettier": "^1.14.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.1.1",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2"
  }
}
